<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beingthere2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/teamwork"
        android:label="Being There"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".teacherList"></activity>
        <activity android:name=".studentList" />
        <activity android:name=".teacher_attendancesheet" />
        <activity android:name=".takeAttendance" />
        <activity android:name=".student_attendance_sheet" />
        <activity android:name=".addteacher" />
        <activity android:name=".addstudent" />
        <activity android:name=".admin_attendanceSheet" />
        <activity android:name=".adminActivity" />
        <activity android:name=".teacherActivity" />
        <activity android:name=".studentActivity" />
        <activity android:name=".loginActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

</manifest>